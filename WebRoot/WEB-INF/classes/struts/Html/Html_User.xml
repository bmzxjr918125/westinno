<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
	"http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="htmluser" extends="westuser">
        <!-- 关于HTML 新闻的相关action -->
        <action name="htmlUser*" class="htmlUserAction" method="{1}">
            <result name="Register">/frontpage/User/register.jsp</result>
            <result name="RegisterSuccess">/frontpage/User/register_success.jsp</result>
            <result name="Login">/frontpage/User/login.jsp</result>
            <result name="LoginOut" type="redirect">login.html</result>
            <result name="MemberCenter">/frontpage/User/member_center.jsp</result>
        </action>
    </package>
</struts>
	