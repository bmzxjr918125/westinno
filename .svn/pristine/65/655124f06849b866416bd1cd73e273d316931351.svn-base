$(".accounts_btn").on("click",function(){
    $(".gap").css({
        display: "block"
    });
    $(".loading").css({
        display: "block"
    });
    setTimeout(function(){
        $(".loading").css({
            display: "none"
        });
        $(".pay_now").css({
            display: "block"
        });
    },3000);
});

$(".cancel_btn").on("click",function(){
    $(".gap").css({
        display: "none"
    });
    $(".pay_now").css({
        display: "none"
    });
});