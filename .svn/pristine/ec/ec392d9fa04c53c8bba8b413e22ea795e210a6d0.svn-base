body{
    background-color: #f2f2f2;
}
.main{
    margin-bottom: 15px;
}
.member_top{
    padding: 12px 0;
    overflow: hidden;
}
.member_top img{
    width: 36px;
    height: 36px;
    float: left;
}
.member_top span{
    font-size: 16px;
    color: #666666;
    line-height: 36px;
    margin-left: 10px;
}
.shop_bar{
    padding: 20px;
    overflow: hidden;
    background-color: #ffffff;
    border-bottom: #cccccc solid 1px;
}
.goods_info{
    height: 30px;
    float: left;
    font-size: 14px;
    color: #666666;
    line-height: 30px;
    text-align: center;
    border-left: #cccccc solid 1px;
}
.goods_info:first-child{
    border-left: none;
}
.shop_bar .goods_name{
    width: 510px;
}
.shop_bar .unit_price{
    width: 169px;
}
.shop_bar .goods_num{
    width: 169px;
}
.shop_bar .total{
    width: 169px;
}
.shop_bar .operate{
    width: 139px;
}
.shop_content{
    min-height: 120px;
    margin: 10px 0 0;
    padding-left: 40px;
    background-color: #ffffff;
    border-bottom: #cccccc solid 1px;
}
.shop_list{
    padding: 20px 0;
    overflow: hidden;
    border-bottom: #cccccc solid 1px;
}
.shop_list:last-child{
    border-bottom: none;
}
.shop_list_left{
    float: left;
    overflow: hidden;
}
.shop_list_pic{
    width: 80px;
    height: 80px;
    float: left;
}
.shop_list_pic img{
    width: 80px;
    height: 80px;
}
.shop_list_name{
    width: 376px;
    font-size: 16px;
    color: #666666;
    line-height: 80px;
    float: left;
    margin-left: 35px;
}
.goods_info_list{
    float: left;
}
.price_num{
    width: 170px;
    font-size: 16px;
    color: #999999;
    line-height: 80px;
    text-align: center;
}
.num{
    width: 170px;
}
.num_main{
    width: 150px;
    height: 40px;
    margin: 19px auto;
    background-color: #f2f2f2;
    border: #999999 solid 1px;
    border-radius: 5px;
}
.num_main img{
    width: 36px;
    height: 36px;
    margin: 2px;
    float: left;
    cursor: pointer;
}
.num_main input{
    width: 70px;
    height: 36px;
    margin: 2px 0;
    float: left;
    font-size: 18px;
    color: #cccccc;
    text-align: center;
    outline: inherit;
    border: 0;
    background-color: #f2f2f2;
    line-height: 36px;
}
.total_num{
    width: 170px;
    font-size: 16px;
    color: #FF9900;
    text-align: center;
    line-height: 80px;
}
.operate_pic{
    width: 140px;
    text-align: center;
}
.operate_pic img{
    width: 36px;
    height: 36px;
    margin: 22px 0;
}
.total_money{
    width: 1160px;
    height: 50px;
    padding: 15px 40px 15px 0;
    text-align: center;
    background-color: #ffffff;
    border-bottom: #cccccc solid 1px;
    margin-top: 10px;
}
.total_money span{
    font-size: 16px;
    color: #666666;
    line-height: 50px;
    float: right;
}
.total_money i{
    width: 170px;
    font-style: normal;
    color: #ff9900;
    font-size: 28px;
    text-align: center;
    line-height: 50px;
    float: right;
}
.total_money button{
    width: 200px;
    height: 50px;
    border: none;
    border-radius: 5px;
    color: #ffffff;
    font-size: 16px;
    float: right;
}

/*快速注册*/
.register_soon_main{
    width: 100%;
    position: fixed;
    top: 150px;
    left: 0;
    display: none;
}
.register_soon{
    width: 570px;
    margin: 0 auto;
    background-color: #ffffff;
    border-radius: 5px;
    box-shadow: 0 5px 5px #333333;
}
.login_head{
    font-size: 22px;
    color: #ff9900;
    text-align: center;
    padding: 25px 0;
    border-bottom: #cccccc solid 1px;
    position: relative;
}
.cancel_pic{
    width: 28px;
    height: 28px;
    position: absolute;
    top: 25px;
    right: 20px;
    cursor: pointer;
}
.cancel_pic img{
    width: 28px;
    height: 28px;
}

.register_content{
    width: 440px;
    margin: 35px auto 0;
}
.login_inp{
    height: 60px;
    margin-bottom: 20px;
}
.login_inp_list{
    height: 28px;
    padding: 15px 20px;
    border-radius: 5px;
    border: #cccccc solid 1px;
    position: relative;
    float: left;
}
.inp_test{
    height: 60px;
    margin-bottom: 20px;
}
.inp_test .login_inp_list{
    width: 190px;
}
.inp_test .login_inp_list input{
    width: 147px;
}
.test_pic{
    width: 170px;
    height: 60px;
    float: left;
    margin-left: 10px;
}
.test_pic img{
    width: 170px;
    height: 60px;
    vertical-align: middle;
}
.test_btn{
    margin-bottom: 20px;
}
.test_btn .btn_moni{
    width: 232px;
    height: 50px;
    outline: inherit;
    border: none;
    border-radius: 5px;
    font-size: 16px;
    color: #ffffff;
    line-height: 50px;
    text-align: center;
    cursor: pointer;
}

.login_icon{
    float: left;
}
.login_icon img{
    width: 28px;
    height: 28px;
}
.input_main{
    float: left;
    margin-left: 15px;
}
.input_main input{
    width: 355px;
    height: 28px;
    font-size: 16px;
    color: #999999;
    line-height: 28px;
    padding: 0;
    margin: 0;
    outline: inherit;
    border: 0;
    background-color: #ffffff;
}
.login_btn{
    text-align: center;
    margin-bottom: 20px;
}
.login_btn button{
    width: 380px;
    height: 50px;
    outline: inherit;
    border: none;
    border-radius: 5px;
    font-size: 16px;
    color: #ffffff;
}

.register_dic{
    font-size: 13px;
    color: #666666;
    text-align: center;
    padding-bottom: 20px;
}
.register_dic a{
    color: #ff9900;
}
