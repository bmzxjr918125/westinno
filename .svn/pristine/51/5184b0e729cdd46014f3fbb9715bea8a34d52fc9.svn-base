body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, p, blockquote, th, td {
    margin:0; padding:0;
}
body {
    font-size:12px;
    color:#666;
    font-family:Verdana, Microsoft YaHei, Simsun;
    background:#fff;
}
fieldset, img {
    border:0;
    vertical-align: middle;
}
ol, ul {
    list-style:none;
}
h1, h2, h3, h4, h5, h6{
    font-size:100%;
}
em {
    font-style:normal;
}
input, button, select, textarea {
    outline:none;
}
textarea {
    resize:none;
}
p{
    text-align:justify; text-justify:distribute;
}
a{
    color: #666; text-decoration:none;
}
a:hover{
    color: #f60; text-decoration:none;
}
body{
    background-color: #f2f2f2;
}
.theme{
    width: 100%;
    height: 105px;
    background-color: #01af21;
}
.theme_content{
    width: 1120px;
    height: 105px;
    margin: 0 auto;
    overflow: hidden;
    text-align: center;
    line-height: 105px;
}
.theme_name{
    float: left;
    width: 210px;
    height: 105px;
    position: relative;
    text-align: center;
}
.theme_name span{
    color: #ffffff;
    font-size: 28px;
}
.theme_name .orange_font{
    color: #ffcc00;
}
.triangle_pic{
    width: 210px;
    height: 16px;
    background-color: #ffffff;
    position: absolute;
    bottom: 0;
    left: 0;
}
.triangle_pic img{
    width: 210px;
    height: 16px;
    position: absolute;
    left: 0;
    top: 0;
}
.yihu_content{
    width: 1120px;
    margin: 0 auto;
    overflow: hidden;
    background-color: #ffffff;
}
.left_nav{
    float: left;
    position: relative;
    width: 210px;
}
.move_piece{
    width: 14px;
    height: 50px;
    background-color: #01af21;
    position: absolute;
    left: 0;
    top: 20px;
}
.nav_list ul li{
    width: 210px;
    height: 50px;
    margin: 20px 0;
    text-align: center;
    cursor: pointer;
}
.nav_list ul li span{
    color: #01af21;
    font-size: 18px;
    line-height: 50px;
}
.main_content{
    width: 910px;
    float: left;
}
/*公司介绍*/
.content_list{
    margin: 20px 30px;
    display: none;
}
.show_current{
    display: block;
}
.content_head{
    text-align: left;
}
.content_head h1{
    color: #01af21;
    font-size: 18px;
    font-weight: 700;
}
.content_words{
    text-indent: 32px;
    margin: 25px 0;
}
.content_words p{
    font-size: 18px;
    color: #999999;
}
.team_member{
    float: left;
    margin-top: 15px;
    overflow: hidden;
}
.team_member_list{
    float: left;
    width: 190px;
    margin-left: 30px;
}
.team_member_list:nth-child(1){
    margin-left: 0;
}
.team_member_list img{
    width: 190px;
    height: 190px;
    border-radius: 5px;
}
.member_name{
    margin-top: 25px;
    text-align: center;
}
.member_name span{
    font-size: 22px;
    color: #01af21;
}
.member_position{
    margin-top: 5px;
    text-align: center;
}
.member_position span{
    font-size: 18px;
    color: #999999;
}

.member_content{
    width: 100%;
    overflow: hidden;
}
.member_content h1{
    width: 100%;
    text-align: center;
    font-weight: normal;
    font-size: 18px;
    color: #01af21;
    margin: 20px 0;
}
.member_list_content{
    overflow: hidden;
    width: 100%;
    float: left;
    margin: 20px 0 50px;
}
.member_list{
    float: left;
    width: 130px;
    margin-left: 14px;
}
.member_list:first-child{
    margin-left: 0;
}
.member_list:last-child .member_list_pic{
    background-color: #01af21;
}
.member_list_pic{
    width: 130px;
    height: 140px;
}
.member_list_pic img{
    width: 130px;
    height: 140px;
    border-radius: 5px;
    vertical-align: middle;
}
.member_list_icon{
    width: 130px;
    height: 140px;
    background-color: #01af21;
}
.member_list_icon img{
    width: 50px;
    height: 50px;
    margin: 45px 40px;
}
.member_name2{
    margin-top: 15px;
    text-align: center;
}
.member_name2 span{
    font-size: 18px;
    color: #999999;
}

/*联系我们*/
.map{
    width: 790px;
    height: 520px;
    margin: 20px auto 45px;
    border: #01af21 solid 1px;
}
.map_pic{
    width: 760px;
    height: 490px;
    margin: 15px;
}
.map_pic img{
    width: 760px;
    height: 490px;
}
.address_head{
    margin: 20px 0;
}
.address_words p{
    font-size: 18px;
    color: #999999;
    margin: 10px 0;
}
.blank_gap{
    width: 100%;
    height: 260px;
}

/*服务条款*/
.clause_top .clause_top_head span{
    font-size: 18px;
    color: #01af21;
}
.indentation{
    text-indent: 32px;
}
.clause_top p{
    color: #999999;
    font-size: 18px;
}
.clause_list{
    margin-top: 25px;
}
.clause_list_head{
    font-size: 18px;
    color: #01af21;
}
.clause_list .indentation{
    color: #999999;
    font-size: 18px;
}
.clause_list .indentation_head{
    font-size: 18px;
    color: #999999;
}

/*合作伙伴*/
.cooperation_partner{
    height: 1000px;
}
.cooperation_partner_list{
    margin-bottom: 50px;
}
.cooperation_partner_head{
    margin-bottom: 10px;
}
.cooperation_partner_head h1{
    font-size: 18px;
    font-weight: 700;
    color: #01af21;
}
.cooperation_partner_list p{
    color: #999999;
    font-size: 18px;
}



















