body{
    background-color: #f2f2f2;
}

.login_main{
    padding: 30px 0;
    border-top: #f2f2f2 solid 1px;
}
.content{
    width: 1200px;
    margin: 0 auto;
    height: 500px;
    background-color: #ffffff;
    border-bottom: #cccccc solid 1px;
}
.login_content{
    width: 440px;
    margin: 0 auto;
}
.login_head{
    font-size: 22px;
    color: #ff9900;
    text-align: center;
    padding: 30px 0;
}
.login_inp{
    height: 140px;
    border: #cccccc solid 1px;
    border-radius: 5px;
}
.login_inp_list{
    height: 28px;
    padding: 21px 20px;
    position: relative;
}
.login_inp_list:first-child{
    border-bottom: #cccccc solid 1px;
}
.login_icon{
    float: left;
}
.login_icon img{
    width: 28px;
    height: 28px;
}
.input_main{
    float: left;
    margin-left: 15px;
}
.input_main input{
    width: 355px;
    height: 28px;
    font-size: 16px;
    color: #999999;
    line-height: 28px;
    padding: 0;
    margin: 0;
    outline: inherit;
    border: none;
    background-color: #ffffff;
}
.auto_login{
    width: 440px;
    overflow: hidden;
    padding: 20px 0;
}
.auto_login_left{
    float: left;
}
.auto_login_right{
    width: 85px;
    float: right;
    text-align: center;
}
.auto_login_left span{
    font-size: 14px;
    color: #666666;
    margin-left: 20px;
}
.auto_login_left input,.auto_login_left span{
    vertical-align: middle;
}
.auto_login_right a{
    font-size: 14px;
    color: #999999;
}
.login_btn{
    text-align: center;
    margin-bottom: 20px;
}
.login_btn button{
    width: 380px;
    height: 50px;
    outline: inherit;
    border: none;
    border-radius: 5px;
    font-size: 16px;
    color: #ffffff;
}
.other_login_head{
    font-size: 14px;
    color: #999999;
    line-height: 22px;
}
.other_way{
    margin: 25px 0;
    overflow: hidden;
}
.other_way_list{
    width: 60px;
    height: 60px;
    float: left;
    margin-left: 30px;
}

.copy{
    margin-top: 10px;
}